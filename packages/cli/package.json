{
  "name": "dotcom-tool-kit",
  "version": "0.0.0",
  "author": "Platforms Team",
  "bin": {
    "dotcom-tool-kit": "./bin/run"
  },
  "bugs": "https://github.com/financial-times/dotcom-tool-kit/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/node": "^10.17.54",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/financial-times/dotcom-tool-kit",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dotcom-tool-kit",
    "plugins": [
      "@oclif/plugin-help",
      "installation-plugins"
    ]
  },
  "repository": "financial-times/dotcom-tool-kit",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "installation-plugins": "file:../installation-plugins",
    "tslib": "^1.14.1"
  }
}
