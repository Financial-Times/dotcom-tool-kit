{
  "name": ebi,
  "version": "0.0.0",
  "description": "A command line tool that searches files within GitHub repositories",
  "bin": "./bin/ebi.js",
  "scripts": {
    "lint": "eslint *.js src/ lib/ test/",
    "lint-fix": "eslint --fix *.js src/ lib/ test/",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "test": "npm run lint && npm run unit-test",
    "heroku-postbuild": "dotcom-tool-kit heroku:postbuild"
  },
  "license": "MIT",
  "engines": {
    "node": "10.15.0"
  },
  "dependencies": {
    "@octokit/plugin-throttling": "^2.4.0",
    "@octokit/rest": "^16.15.0",
    "lodash": "^4.17.11",
    "nock": "^10.0.6",
    "yargs": "^13.1.0"
  },
  "devDependencies": {
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-no-only-tests": "^2.1.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "prettier": "1.16.3"
  }
}
